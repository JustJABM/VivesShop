@{
    ViewData["Title"] = "Bestelling";
}


<div>
    <div style='float: left;' class="table-bordered">
        <h1>Arikelen</h1>
        <table class="table-bordered">
            <thead>
                <tr>

                    <td>Artikel</td>
                    <td>Soort</td>
                    <td>Prijs</td>
                    <td>Toevoegen</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var items in Model)
                {
                    <partial name="ItemPartial" model="items" />
                }
            </tbody>
        </table>
    </div>
    <div style='float:right;' class="table-bordered">
        <h1>Bestelling</h1>
        @* @foreach (var orders in Model)
        {
            <partial name="OrderPartial" model="orders" />
        }
        <a asp-action="Create" class="btn btn-info">Bestel</a>*@

    </div>
</div>