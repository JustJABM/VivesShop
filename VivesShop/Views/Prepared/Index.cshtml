
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<div class="table-responsive">
    <div style='float: left;' class="table-bordered">
        <h1>Arikelen</h1>
        <table class="table-bordered">
            <thead>
            <tr>

                <td>Artikel</td>
                <td>Soort</td>
                <td>Prijs</td>
                <td>Acties</td>
            </tr>
            </thead>
            <tbody>
            @foreach (var prepareds in Model.OrderPrepared)
            {
                <partial name="ItemPartial" model="prepareds" />
            }
            </tbody>
        </table>
    </div>
</div>

